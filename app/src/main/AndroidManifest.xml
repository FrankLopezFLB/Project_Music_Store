<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.desarrollo.project_music_store">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project_Music_Store.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".ConsultaServicioActivity"></activity>
        <activity android:name=".ListadoServiciosActivity" />
        <activity android:name=".ConsultaProductoActivity" />
        <activity android:name=".RegistrarServicioActivity"></activity>
        <activity android:name=".ListadoProductoActivity" />
        <activity android:name=".HabilitarTrabajadorActivity" />
        <activity android:name=".CanastaActivity" />
        <activity android:name=".RegistrarProductoActivity" />
        <activity android:name=".RedirectActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

	<activity android:name=".MantenimientoClienteActivity" />
	<activity android:name=".NuevoClienteActivity" />
	<activity android:name=".BuscarClienteActivity"/>
	<activity android:name=".ListaClientesActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".MiPerfilActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".HistorialComprasActivity" />
    </application>

</manifest>